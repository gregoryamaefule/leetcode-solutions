import math 

class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        res = []
        for i in range(numRows + 1):
            i_res = []
            for j in range(1, i+1):
                i_res.append(math.comb(i-1, j-1))
            res.append(i_res)
        return res[1:]
